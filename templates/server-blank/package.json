{
    "name": "@httpc/template-server-blank",
    "description": "Black template based on @httpc/server",
    "title": "Server blank",
    "templateType": "server",
    "version": "0.1.0",
    "main": "dist/index.js",
    "license": "MIT",
    "scripts": {
        "start": "node dist/index.js",
        "dev": "ts-node-dev --transpile-only src/index.ts",
        "build": "tsc",
        "generate:client": "httpc client generate"
    },
    "engines": {
        "node": ">=16.13"
    },
    "dependencies": {
        "@httpc/server": "workspace:^0.1.0"
    },
    "devDependencies": {
        "@httpc/cli": "workspace:^0.1.0",
        "@types/node": "^16.18.11",
        "ts-node-dev": "^2.0.0",
        "typescript": "^4.9.4"
    },
    "httpc": {
        "name": "client",
        "entry": "src/calls/index.ts",
        "dest": "client"
    }
}
