---
import { getDocs } from "../content";

const items = (await getDocs()).filter(x => x.url.startsWith("/docs/adapters/"));
---

<ul>
  {
    items.map(x => (
      <li>
        <p>
          <a href={x.url}>{x.frontmatter.title}</a>
        </p>
        <small>{x.frontmatter.description}</small>
      </li>
    ))
  }
</ul>
